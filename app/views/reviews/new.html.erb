

<div class="container m-3">
  <%= simple_form_for [ @restaurant, @review ] do |f| %>
      <%= f.input :content, as: :text %>
      <%= f.input :rating, as: :integer %>
      <%= f.submit "Submit review", class: "btn btn-primary" %>
  <% end %>
  <hr>
   <%= link_to "Home", restaurants_path %>
</div>

